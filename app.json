{
  "expo": {
    "name": "Blueace India",
    "slug": "blueace-india",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ic_launcher-web.png",
    "platforms": [
      "android",
      "ios",
      "web"
    ],
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/ic_launcher-web.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/ic_launcher-web.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ic_launcher-web.png",
        "backgroundColor": "#58bcfb"
      },
      "icon": "./assets/ic_launcher-web.png",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "package": "com.hbs.blueacea"
    },
    "web": {
      "favicon": "./assets/ic_launcher-web.png"
    },
    "plugins": [
      "expo-audio"
    ],
    "extra": {
      "playStoreIcon": "./assets/playstore-icon.png",
      "eas": {
        "projectId": "7d5b7a1a-6edb-4415-8822-663fa679c9fa"
      }
    }
  }
}
